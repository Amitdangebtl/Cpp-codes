#include<bits/stdc++.h>
using namespace std;

int CountPositiveNumbers(int arr[],int n)
{
    int count = 0;
    for(int i = 0;i<n;i++)
    {
        if(arr[i]>0)
        {
            count++;
        }
    }
    return count;
}
int main()
{
    int n;
    cout<<"Enter Size of Array : ";
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cout<<CountPositiveNumbers(arr,n);
    return 0;
}
