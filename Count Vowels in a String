#include<bits/stdc++.h>
using namespace std;

int CountVowelsInString(string & s)
{
    int count= 0;
    for(int i = 0;i<s.length();i++)
    {
        char ch = tolower(s[i]);
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
        {
            count++;
        }
    }
    return count;
}
int main()
{
    string s;
    cout<<"Enter Your String  :" ;
    getline(cin,s);
    cout<<CountVowelsInString(s);
    return 0;
}
