#include<bits/stdc++.h>
using namespace std;

void RepleceZeroWithOnes(int arr[],int n)
{
    for(int i =0 ;i<n;i++)
    {
        if (arr[i]==0)
        {
            arr[i]=1;
        }
    }
}
int main()
{
    int n;
    cout<<"Enter Array size : ";
    cin>>n;
    int arr[n];
    cout<<"Enter Arrray Elementes  :" ;
    for(int i =0;i<n;i++)
    {
        cin>>arr[i];
    }
    RepleceZeroWithOnes(arr,n);
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }
}
