#include<bits/stdc++.h>
using namespace std;

int SumOfArray(int arr[],int n)
{
    int sum = 0;
    for(int i = 0;i<n;i++)
    {
        sum += arr[i];
    }
    return sum;
}
int main()
{
    int n;
    cout<<"Enter NUmber of size array : ";
    cin>>n;
    int arr[n];
    cout<<"Enter Array Elementes : ";
    for(int i = 0;i<n;i++)
    {
        cin>>arr[i];
    }
    cout<<SumOfArray(arr, n);
    return 0;
    
}
